{
  "name": "react-use-table-query-params",
  "description": "A simple React library for managing table filters and pagination through the URL",
  "version": "0.0.2",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "author": "@tastaur",
  "license": "Unlicense",
  "sideEffects": false,
  "files": [
    "lib/",
    "esm/"
  ],
  "keywords": [
    "react",
    "hooks",
    "typescript",
    "query-params",
    "pagination",
    "filters"
  ],
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tastaur/react-use-table-query-params"
  },
  "bugs": {
    "url": "https://github.com/Tastaur/react-use-table-query-params/issues"
  },
  "scripts": {
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "pnpm build:cjs && pnpm build:es",
    "lint:eslint": "eslint . --ext .ts,.tsx,.js",
    "lint:dev": "eslint . --ext .ts,.tsx,.js --cache --fix",
    "test": "vitest run",
    "test:ci": "vitest run --coverage --reporter=default --reporter=junit --outputFile=reports/junit.xml"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "vite": "^5.4.9",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@testing-library/react": "16.0.1",
    "babel-preset-vite": "^1.1.3",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jsdom": "24.1.0",
    "vitest": "^2.0.5"
  }
}
